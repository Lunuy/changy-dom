{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "ES2019",
      "declaration": true,
      "rootDir": "./src/",
      "esModuleInterop": true,
      "jsx": "react",
      "jsxFactory": "ChangyDom.createElementJSX",
      "allowSyntheticDefaultImports": true
    },
    "include": [
      "./src/*",
      "./src/**/*"
    ]
}